Content-Type: text/ecl
Entry-Name: .content

importProject -projectName "documentationExampleProject"

with [get-view "Axon Ivy Projects" | get-tree] {
	select "documentationExampleProject/Definitions/Web Service Clients" | double-click
}

proc wsScreenshot [val path]
{
	let [val basePath "designer-configuration"]
	{
		captureScreenshot [format "%s/%s" $basePath $path ]
	}
}

get-eclipse-window | resizeWindow 1000 880
with [get-editor documentationExampleProject] {
    maximize
	get-section "Web Service Clients" | get-tree | select "Web Services/Technical/GeoIpService"
    get-tab-folder | get-object | wsScreenshot "webservice-client-editor.png"

	get-section Authentication | get-object | wsScreenshot "webservice-client-auth.png"
	get-section Authentication | click
	get-section Features | get-object | wsScreenshot "webservice-client-features.png"
	get-section Features | click
	get-section Properties | get-object | wsScreenshot "webservice-client-properties.png"
	get-section Properties | click

    // expand select
    with [get-section "Endpoint URI"] {
        click
        get-list | select GeoIPServiceSoap12
        get-object | wsScreenshot "webservice-client-endpoint-uris.png"
    }
    
    get-section "Web Service Clients" | get-button "Add Environment" | click
    wait 2000 // window will be faded in
    with [get-window "Add Environment"] {
    	get-object | wsScreenshot "webservice-client-environment.png"
    	get-button Cancel | click
    }
}
get-editor documentationExampleProject | close

