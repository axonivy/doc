let [val acmeWebShop "Acme_web_shop"]
{
	createProject -projectName $acmeWebShop
	try{
		get-eclipse-window | resizeWindow 1050 550
		with [get-view "Axon Ivy Projects" | get-tree]{
			select [concat $acmeWebShop "/Config/overrides.any"] | double-click
			get-object | captureScreenshot "designer-configuration/overrides-treenode.png"
		}
	} -finally{
		deleteProject $acmeWebShop
	}
}
