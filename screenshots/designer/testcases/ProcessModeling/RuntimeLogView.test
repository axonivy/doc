Content-Type: text/ecl
Entry-Name: .content

importProject -projectName "documentationExampleProject"

get-button "Start Engine and show overview page" | click
OpenView "Runtime Log"
get-view "Runtime Log.*" | get-button "Clear all log entries" | click
get-view "Web Browser" | get-combo | set-text "http://localhost:8081/designer/pro/documentationExampleProject/1567F0BE1CC43932/start.ivp"
get-view "Web Browser" | get-button "Go to the selected URL" | click

OpenView "Runtime Log"
get-eclipse-window | resizeWindow 1100 600
find-tab-folder [format "Runtime Log (%s)" [get-view "Runtime Log .*" | get-table | get-property itemCount -raw]] | get-object
	| captureScreenshot "simulation-debugging/runtimelog-view.png"

get-view "Runtime Log .*" | get-table | select [get-item "HTTP GET runtimeLogProcess.*\\/.*"
	-column Request -index 5] | double-click

with [get-window "Event Details"] {
	wait 500
	get-control Any -index 0| invoke getParent | get-object | 
	captureScreenshot "simulation-debugging/runtimelog-view-details.png"
	get-button "View Details Of Previous Event" | get-object | captureScreenshot "simulation-debugging/runtimelog-view-details-button-previous-event.png"
	get-button "View Details Of Next Event" | get-object | captureScreenshot "simulation-debugging/runtimelog-view-details-button-next-event.png"
	get-button "Goto User Dialog view" | get-object | captureScreenshot "simulation-debugging/runtimelog-view-details-button-goto-userdialog-view.png"
	get-button "Goto process element" | get-object | captureScreenshot "simulation-debugging/runtimelog-view-details-button-goto-process-element.png"
	get-button "Copy Event Details To Clipboard" | get-object | captureScreenshot "simulation-debugging/runtimelog-view-details-button-copy-event-details.png"
	get-button "Save Error Report" | get-object | captureScreenshot "simulation-debugging/runtimelog-view-details-button-save-error-report.png"
	close
}
OpenView "Process Performance"
wait 500
find-tab-folder "Process Performance" | get-object | captureScreenshot "simulation-debugging/process-performance-view.png"
