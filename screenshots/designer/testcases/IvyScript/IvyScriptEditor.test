proc takeIvyscriptScreenshot [val label] [val image] {
	get-editbox -after [get-label $label] | key-type END
	get-control Any -after [get-label Icon] -index 10 | get-object | captureScreenshot [format "ivyscript/%s.png" $image]
}

importProject documentationExampleProject

let [val processName IvyScriptProcess] [val elementName "Notify Customer"] [val elementType "User Task"] {
	openInscription -processName $processName -elementName $elementName -elementType $elementType

	with [get-window "Inscribe User Task"] {
		resizeWindow 730 730
		get-tab-folder | get-tab-item Task | click
		takeIvyscriptScreenshot Name "macro-editor"
		takeIvyscriptScreenshot Responsible "ivy-script-editor"
		get-button Cancel | click
	}
}
