Content-Type: text/ecl
Entry-Name: .content

importProject "documentationExampleProject"
importProject "QuickStartTutorial"
togglePresentation -flat true

try {
	with [get-view "Axon Ivy Projects" | get-tree] {
		select "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | get-menu Move | click
	}
	with [get-window Refactoring] {
		resizeWindow 650 450
		saveWindowContentScreenshot "designer/refactoring-move-wizard.png"
		get-button "Next >" | click
		wait -ms 500
		saveWindowContentScreenshot "designer/refactoring-broken-references-page.png"
		get-button "Next >" | click
		resizeWindow 650 450
		saveWindowContentScreenshot "designer/refactoring-overview-page.png"
		get-button Cancel | click
	}
	
	with [get-view "Axon Ivy Projects" | get-tree] {
		get-item "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | click
		get-item "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | click
		select "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | get-menu Rename | click
	}
	with [get-window Refactoring] {
		resizeWindow 650 315
		get-editbox -after [get-label "Name:"] | set-text exampleHtmlDialog1
		saveWindowContentScreenshot "designer/refactoring-rename-wizard.png"
		get-button Cancel | click
	}
	
	with [get-view "Axon Ivy Projects" | get-tree] {
		get-item "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | click
		get-item "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | click
		select "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | get-menu Delete | click
	}
	with [get-window Refactoring] {
		resizeWindow 650 400
		saveWindowContentScreenshot "designer/refactoring-delete-wizard.png"
		get-button Cancel | click
	}
	
	with [get-view "Axon Ivy Projects" | get-tree] {
		get-item "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | click
		select "documentationExampleProject/Html Dialogs/screenshot.project.exampleHtmlDialog" | get-menu Copy | click
		get-item "documentationExampleProject/Html Dialogs" | click
		select "documentationExampleProject/Html Dialogs" | get-menu Paste | click
	}
	with [get-window Refactoring] {
		resizeWindow 650 300
		saveWindowContentScreenshot "designer/refactoring-copy-wizard.png"
		get-button Cancel | click
	}
} -finally {
    deleteAllProjects
}
