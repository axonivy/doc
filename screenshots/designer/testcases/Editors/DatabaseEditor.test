Content-Type: text/ecl
Entry-Name: .content

importProject -projectName "documentationExampleProject"

with [get-view "Axon Ivy Projects" | get-tree] {
	select "documentationExampleProject/Configuration/databases.yaml" | double-click
}
get-eclipse-window | resizeWindow 1050 850
with [get-editor documentationExampleProject] {
	maximize
	get-section "All Database Configurations" | get-tree | select "ERP"
	get-tab-folder | get-object | captureScreenshot "designer-configuration/database-editor.png"
	
	get-section "All Database Configurations" | get-button "Add Environment" | click
	
	waitWindowExists "Add Environment"
	
	with [get-window "Add Environment"]
	{
		get-object | captureScreenshot "designer-configuration/database-environment.png"
		get-button "Cancel" | click
	}
}
get-editor documentationExampleProject | close
