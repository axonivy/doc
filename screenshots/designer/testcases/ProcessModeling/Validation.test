Entry-Name: .content

importProject -projectName "documentationExampleProject"
togglePresentation -flat true

try {
	get-view "Axon Ivy Projects" | get-tree | select "documentationExampleProject/Html Dialogs/screenshot.project.example" +
		"HtmlDialog" | get-menu Delete | click
	get-window Refactoring | get-button Finish | click
	get-view Problems | click
	wait 1000
	
	get-view "Axon Ivy Projects" | get-tree | select documentationExampleProject | get-menu Validate | click
	get-window "Validation Results" | close
	OpenView Problems
	get-view Problems | get-tree | select "Errors .*/User Dialog 'screenshot.project.exampleHtmlDialog' not found."
	
	get-eclipse-window | resizeWindow 1300 800
	find-tab-folder Problems | get-object | captureScreenshot "ivy-project/validation-problems-view.png"
} -finally {
    deleteProject "documentationExampleProject"
}
