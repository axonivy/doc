importProject "documentationExampleProject"

proc capture [val elementName] [val elementType] [val tabName] [val screenshotName] [val width] [val height] {
	let [val processName "intermediateEventElements"] {
		openInscription
			-processName $processName
			-elementName $elementName
			-elementType $elementType
			
		captureInscription
			-elementType $elementType
			-tabName $tabName
			-width $width -height $height
			-screenshot $screenshotName
	}
}


let [val elementName "read address"] [val elementType "Program Intermediate Event"] {
	capture $elementName $elementType "Task" "waitIntermediateEvent_taskTab.png" 650 400
	capture $elementName $elementType "Event" "waitIntermediateEvent_eventTab.png" 650 600
	capture $elementName $elementType "Editor" "waitIntermediateEvent_editorTab.png" 650 600
}

let [val elementName "erpUpdate"] [val elementType "Program Start"] {
	openInscription -processName "intermediateEventElements"
		-elementName $elementName
		-elementType $elementType
	with [get-window [format "Inscribe %s" $elementType]] {
		get-tab-folder | get-tab-item Start | click
		get-section Permission | click // expand
	}
	captureInscription -elementType $elementType -tabName "Start" -screenshot "programStart_startTab.png" -width 650 -height 400 -close false
	captureInscription -elementType $elementType -tabName "Editor" -screenshot "programStart_editorTab.png" -width 650 -height 400
}

let [val elementName "load ERP"] [val elementType "Program Interface Activity"] {
	openInscription -processName "intermediateEventElements"
		-elementName $elementName
		-elementType $elementType
	captureInscription -elementType $elementType -tabName "Start" -screenshot "programInterface_startTab.png" -width 650 -height 400 -close false
	captureInscription -elementType $elementType -tabName "Editor" -screenshot "programInterface_editorTab.png" -width 650 -height 400
}


