importProject "documentationExampleProject"

let [val processName "taskEventAndGateway"][val elementName "Verify Request"][val elementType "User Task"][val windowName "Inscribe User Task"][val tabName "Task"]
{
	openInscription
		-processName $processName
		-elementName $elementName
		-elementType $elementType

	captureInscription
		-elementType $elementType
		-tabName $tabName
		-width 650 -height 500
		-screenshot "tab-task.png"

	//Options expander
	openInscription
		-processName $processName
		-elementName "Reset password"
		-elementType $elementType
	with [get-window $windowName]
	{
		resizeWindow -width 600 -height 600

		get-tab-folder | get-tab-item $tabName | click
    	with [get-section "Options"] {
        	captureInscriptionPart -screenshot "tab-task-options.png"
    	}
	    get-button Cancel | click
	}

	//Expiry expander
	openInscription
		-processName $processName
		-elementName "Escalate to manager"
		-elementType $elementType
	with [get-window $windowName]
	{
		resizeWindow -width 553 -height 600

		get-tab-folder | get-tab-item $tabName | click
    	with [get-section "Expiry"] {
        	captureInscriptionPart -screenshot "tab-task-expiry.png"
    	}
	    get-button Cancel | click
	}

	//Custom Fields expander
	openInscription
		-processName $processName
		-elementName "Escalate to manager"
		-elementType $elementType
	with [get-window $windowName]
	{
		resizeWindow -width 553 -height 800

		get-tab-folder | get-tab-item $tabName | click
    	with [get-section "Custom Fields"] {
        	captureInscriptionPart -screenshot "tab-task-custom-fields.png"
    	}

		resizeWindow -width 670 -height 790
    	with [get-section "Code"] {
    		captureInscriptionPart -screenshot "tab-task-code.png"
    	}
	    get-button Cancel | click
	}

	openInscription
		-processName $processName
		-elementName $elementName
		-elementType $elementType
	captureInscription
		-elementType $elementType
		-tabName "Call"
		-width 650 -height 600
		-screenshot "user-task-tab-call.png"
}
