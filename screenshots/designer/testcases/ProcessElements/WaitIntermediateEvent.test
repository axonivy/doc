importProject "documentationExampleProject"

proc capture [val elementName] [val elementType] [val tabName] [val screenshotName] [val width] [val height]
{
	let [val processName "intermediateEventElements"]
	{
		openInscription
			-processName $processName
			-elementName $elementName
			-elementType $elementType
			
		captureInscription
			-elementType $elementType
			-tabName $tabName
			-width $width -height $height
			-screenshot $screenshotName
	}
}

