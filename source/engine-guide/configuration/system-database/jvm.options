# path to the key store that holds the AES secret key
-Dch.ivyteam.ivy.persistence.keystore.file=keystore.jceks

# password needed to read the key store file
-Dch.ivyteam.ivy.persistence.keystore.password=changeit

# name of the key to read from the key store file
-Dch.ivyteam.ivy.persistence.keystore.alias=aes

# type of the key store        
-Dch.ivyteam.ivy.persistence.keystore.type=jceks
