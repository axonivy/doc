importProject -projectName "documentationExampleProject"

with [get-view "Axon.ivy Projects" | get-tree] {
	get-item "documentationExampleProject/Processes" | click
	select "documentationExampleProject/Processes" | get-menu "New/Process" | click
}
with [get-window "New Process"] {
	resizeWindow 550 330
	get-group Process | get-editbox -after [get-label "Name:"] | set-text AddContact
	wait 500
	saveWindowContentScreenshot "process-modeling/new-process-wizard-page-1.png"
	get-button "Next >" | click
	wait 500
	saveWindowContentScreenshot "process-modeling/new-process-wizard-page-2-normal.png"
	get-button Cancel | click
}

with [get-view "Axon.ivy Projects" | get-tree] {
	get-item "documentationExampleProject/Processes" | click
	select "documentationExampleProject/Processes" | get-menu "New/Process" | click
}
with [get-window "New Process"] {
	resizeWindow 550 470
	with [get-group Process] {
		get-button "Callable Sub Process" | click
		get-editbox -after [get-label "Name:"] | set-text AddContact
	}
	get-button "Next >" | click
	get-group Parameters | get-button "Select an existing data class for the process." | click
	with [get-window "Data Class Selection"] {
		get-editbox -after [get-label "Choose an ivy data class.Enter part of type name to search, use * and ? as wildcards."] | set-text "ch."
		get-table | select "AddContactData - ch.ivyteam.documentation.project.*"
		get-button OK | click
	}
	get-group Parameters | get-tree | select "addContactData/addPerson"
	wait 500
	saveWindowContentScreenshot "process-modeling/new-process-wizard-page-2-mapping.png"
	get-button Finish | click
}

with [get-view "Axon.ivy Projects" | get-tree] {
	get-item "documentationExampleProject/Processes" | click
	select "documentationExampleProject/Processes" | get-menu "New/Process Group" | click
}
with [get-window "New Process Group"] {
	resizeWindow 520 330
	get-editbox -after [get-label "Process Group Name:"] | set-text Main
	wait 500
	saveWindowContentScreenshot "process-modeling/new-process-group-wizard.png"
	get-button Cancel | click
}
